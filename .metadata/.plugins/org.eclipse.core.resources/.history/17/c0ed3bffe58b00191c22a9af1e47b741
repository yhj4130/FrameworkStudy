package com.test.spr;

import java.lang.reflect.InvocationHandler;

// ※ Proxy 클래스를 만드는 방법 중 비교적 쉽고 직관적인 방법은
//    InvocationHandler 인터페이스를 구현하는 클래스를 만드는 것이다.

public class CalculatorProxy implements InvocationHandler
{
	
}
